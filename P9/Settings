# -*- makefile -*-
#
#  Project Root
#

##
##  Configuration
##

# Path to cxxtest
CXXTESTDIR = $(CONFIG_AFL_CXXTESTDIR)

# Path to perl executable
PERL = $(CONFIG_AFL_PERL)

# C++ compiler and flags
CXX = $(CONFIG_AFL_CXX)
CXXFLAGS = $(CONFIG_AFL_CXXFLAGS) -I.

# Libraries and linker flags
LIBS = $(CONFIG_AFL_LIBS)
LDFLAGS = $(CONFIG_AFL_LDFLAGS)

# Additional dependencies that trigger a relink
LIBDEPEND = $(CONFIG_AFL_LIBDEPEND)

# Librarian
AR = $(CONFIG_AFL_AR)

# "rm" tool
RM = rm

# A program to execute a compiled program. Empty for native execution, but could be something like a simulator.
RUN = $(CONFIG_AFL_RUN)

include config.mk

# This defines the Makefiles we want to make:
GENERATE = Makefile Makefile.bor Makefile.min
INCLUDE_Makefile = Rules-unix.mak Settings-unix.mak
INCLUDE_Makefile.bor = Rules-win32.mak Settings-win32.mak
INCLUDE_Makefile.min = Rules-mingw.mak Settings-mingw.mak

PROJ_AUTO += afl:afl/*pp testsuite:u/*pp


